  "version": "0.2.0",
  "configurations": [
    {
      "type": "gdb", // Change this to gdb if you are on linux
      "request": "launch",
      "name": "Vector string",
      "program": "${workspaceFolder}/unit_test_vector_string.bin",
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "gdb",
      "request": "launch",
      "name": "Vector char",
      "program": "${workspaceFolder}/unit_test_vector_char.bin",
      "args": ["input.txt"],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "gdb",
      "request": "launch",
      "name": "Grade tokenize",
      "program": "${workspaceFolder}/tokenize.bin",
      "args": ["txt/input.txt"],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "gdb",
      "request": "launch",
      "name": "Grade dictionary",
      "program": "${workspaceFolder}/dictionary.bin",
      "args": ["txt/input"],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "gdb",
      "request": "launch",
      "name": "Word count",
      "program": "${workspaceFolder}/wordcount.bin",
      "args": ["txt/input1.txt"],
      "cwd": "${workspaceFolder}"
    }
  ]
}
